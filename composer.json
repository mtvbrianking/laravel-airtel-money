{
    "name": "bmatovu/laravel-airtel-money",
    "description": "Laravel Airtel Money API Integration",
    "keywords": [
        "bmatovu",
        "laravel",
        "airtel-money"
    ],
    "homepage": "https://github.com/bmatovu/laravel-airtel-money",
    "license": "MIT",
    "authors": [
        {
            "name": "mtvbrianking",
            "email": "mtvbrianking@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/container": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "nesbot/carbon": "^2.62|^3.0",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0",
        "ramsey/uuid": "^4.7",
        "symfony/http-foundation": "^6.2|^7.0"
    },
    "require-dev": {
        "code-lts/doctum": "^5.5",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/pint": "^1.20",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^10.0|^11.0|^12.0",
        "pestphp/pest": "^2.0|^3.0|^4.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bmatovu\\AirtelMoney\\": "src/",
            "Bmatovu\\AirtelMoney\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bmatovu\\AirtelMoney\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse --level 8",
        "docs": "doctum.php update doctum.php -v",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bmatovu\\AirtelMoney\\AirtelMoneyServiceProvider"
            ],
            "aliases": {
                "Kyc": "Bmatovu\\AirtelMoney\\Facades\\Kyc",
                "Account": "Bmatovu\\AirtelMoney\\Facades\\Account",
                "Collection": "Bmatovu\\AirtelMoney\\Facades\\Collection",
                "Disbursement": "Bmatovu\\AirtelMoney\\Facades\\Disbursement",
                "Authorization": "Bmatovu\\AirtelMoney\\Facades\\Authorization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}